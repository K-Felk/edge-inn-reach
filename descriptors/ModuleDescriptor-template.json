{
  "id": "${artifactId}-${version}",
  "name": "INN-REACH system Edge API module",
  "provides": [
    {
      "id": "_tenant",
      "version": "2.0",
      "interfaceType": "system",
      "handlers": [
        {
          "methods": ["POST"],
          "pathPattern": "/_/tenant"
        }, {
          "methods": ["GET","DELETE"],
          "pathPattern": "/_/tenant/{id}"
        }
      ]
    }
  ],
  "permissionSets" : [],
  "requires": [
    {
      "id": "mod-inn-reach",
      "version": "1.0"
    },
    {
      "id": "login",
      "version": "5.0 6.0 7.0"
    }
  ],
  "launchDescriptor": {
    "dockerImage": "${artifactId}:${version}",
    "dockerPull": false,
    "dockerArgs": {
      "HostConfig": {
        "Memory": 268435456,
        "PortBindings": { "8081/tcp": [ { "HostPort": "%p" } ] }
      }
    },
    "env": [
      { "name": "JAVA_OPTIONS",
        "value": "-XX:MaxRAMPercentage=66.0"
      }
    ]
  }
}
